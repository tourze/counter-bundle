{
  "name": "tourze/counter-bundle",
  "description": "Symfony Counter Bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/common": "^3.5",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/symfony-lock-command-bundle": "0.0.*",
    "tourze/symfony-cron-job-bundle": "0.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  },
  "autoload": {
    "psr-4": {
      "CounterBundle\\": "src"
    }
  }
}